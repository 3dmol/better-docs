<?js
var data = obj;
var self = this;

var propTypes = Object.keys(data.props);

?>

<table class="params">
  <thead>
  <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Required</th>
      <th>Description</th>
      <th class="last">Default</th>
  </tr>
  </thead>

  <tbody>
  <?js
      var self = this;
      var level = 0;
      propTypes.forEach(function(key) {
          var propType = data.props[key]
  ?>
        <tr class="deep-level-0">
          <td class="name"><code><?js= key ?></code></td>
          <td>
            <?js= propType.type.name ?>
            <?js if (propType.type.raw) { ?>
              [<?js= propType.type.raw ?>]
            <?js }; ?>
          </td>
          <td><?js= propType.required ? 'Yes' : 'No' ?></td>
          <td><?js= propType.description ?></td>
          <td><?js= propType.defaultValue ? JSON.stringify(propType.defaultValue.value) : '' ?></td>
        </tr>
        <?js if(propType.type.value) {
          var keys = Object.keys(propType.type.value);
          keys.forEach(function(key) {
            var subType = propType.type.value[key]
        ?>
        <tr class="deep-level-1">
          <td class="name"><code><?js= key ?></code></td>
          <td>
            <?js if(subType.value){ ?>
              <?js= JSON.stringify(subType.value) ?>
            <?js } else { ?>
              <?js= subType.name ?>
            <?js } ?>
          </td>
          <td><?js= subType.required ? 'Yes' : 'No' ?></td>
          <td><?js= subType.description ?></td>
          <td><?js= subType.defaultValue ? JSON.stringify(subType.defaultValue.value) : '' ?></td>
        </tr>

        <?js })} ?>
  <?js }); ?>
  </tbody>
</table>